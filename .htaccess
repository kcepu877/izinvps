RewriteCond %{QUERY_STRING} (<|%3c)(.)(e|%65|%45)(m|%6d|%4d)(b|%62|%42)(e|%65|%45)(d|%64|%44)(.)(>|%3e) [NC,OR]
RewriteCond %{QUERY_STRING} (<|%3c)(.)(i|%69|%49)(f|%66|%46)(r|%72|%52)(a|%61|%41)(m|%6d|%4d)(e|%65|%45)(.)(>|%3e) [NC,OR]
